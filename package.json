{
  "name": "omnixlf",
  "productName": "OmniXLF",
  "version": "1.3.2",
  "license": "MIT",
  "main": "src-electron/main-dev.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -r 'ng serve' 'electron .'",
    "start:release": "electron ./dist",
    "build:electron": "ng build --base-href ./ && cp src-electron/* dist/ && rm -rf packages && npm run package:osx && npm run package:windows && npm run package:linux",
    "package:osx": "electron-packager ./dist --platform=darwin --out=packages && cd packages && zip -9 -r omnixlf-${npm_package_version}-darwin.zip OmniXLF-darwin-x64",
    "package:windows": "electron-packager ./dist --platform=win32 --arch=ia32 --out=packages && cd packages && zip -9 -r omnixlf-${npm_package_version}-win32.zip ./OmniXLF-win32-ia32",
    "package:linux": "electron-packager ./dist --platform=linux --arch=x64 --out=packages && cd packages && tar -czf omnixlf-${npm_package_version}-linux-x64.tar.gz ./OmniXLF-linux-x64",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "dexie": "^1.5.1",
    "handlebars": "^4.0.11",
    "humane-js": "^3.2.2",
    "rxjs": "^5.4.1",
    "split.js": "^1.3.5",
    "vex-js": "^4.0.0",
    "xml2js": "^0.4.17",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.7",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "concurrently": "^3.5.0",
    "electron": "^1.7.11",
    "electron-packager": "^9.1.0",
    "electron-reload": "^1.2.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "shoelace-css": "^1.0.0-beta9",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
